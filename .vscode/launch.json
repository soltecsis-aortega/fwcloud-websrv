  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "runtimeVersion": "14.15.1",
      "request": "launch",
      "name": "Buid & Run FWCloud Websrv",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/dist/src/main.js",
      "preLaunchTask": "npm: build",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ]
    },
    {
      "type": "node",
      "runtimeVersion": "14.15.1",
      "request": "launch",
      "name": "FWCloud-Websrv",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/dist/src/main.js",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Tests",
      "program": "${workspaceRoot}/node_modules/jest/bin/jest",
      "outputCapture": "std",
      "env": {"NODE_ENV": "test"},
      "cwd": "${workspaceRoot}",
    },

  ]
}